@model IEnumerable<Oblig1.Models.Hus>

<div class="row row-cols-1 row-cols-md-3 g-4">
    @foreach (var item in Model)
    {
        <div class="col">
            <div>
                <img src="@item.bildeURL" class="card-img-top" alt="Not loading :(">
                <div>
                    <div class="d-flex justify-content-between mt-2">
                        <h5 class="text-start">
                            @item.Addresse
                        </h5>
                        <h5 class="text-nowrap">
                            @item.Pris.ToString("0.00 NOK")
                        </h5>
                    </div>
                </div>
            </div>
        </div>
    }
</div>